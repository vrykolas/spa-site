{
  "name": "serviceworks-site",
  "version": "3.0.0",
  "scripts": {
    "build": "webpack -p",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "start": "webpack-dev-server --progress --colors --content-base public/ --inline",
    "test": "chimp tests/chimp.local.js",
    "test:lint": "npm run test:lint:js || npm run test:lint:sass",
    "test:lint:js": "eslint src/js tests",
    "test:lint:sass": "sass-lint -c .sass-lint.yml 'src/sass/**/*.scss' -v -q"
  },
  "pre-commit": {
    "run": [
      "precommit-msg",
      "test:lint:js",
      "test:lint:sass"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "chimp": "^0.40.4",
    "copy-webpack-plugin": "^3.0.1",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-mocha": "^4.5.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "file-loader": "^0.9.0",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.3.0",
    "html-minify-loader": "^1.1.0",
    "imagemin-webpack-plugin": "^1.0.8",
    "node-sass": "^3.8.0",
    "pre-commit": "^1.1.3",
    "sass-lint": "^1.9.1",
    "sass-loader": "^4.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone.marionette": "^3.0.0",
    "bootstrap-sass": "^3.3.7",
    "jquery": "^1.12.4",
    "underscore": "^1.8.3"
  }
}
